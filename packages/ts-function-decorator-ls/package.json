{
  "name": "ts-function-decorator-ls",
  "version": "0.0.0",
  "description": "TypeScript language service plugin for handling decorators.",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gensx-inc/ts-function-decorator.git",
    "directory": "packages/ts-function-decorator-ls"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "ai",
    "decorator",
    "language-service",
    "typescript"
  ],
  "author": "GenSX Team",
  "license": "Apache-2.0",
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.1.3",
    "vitest": "^3.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
